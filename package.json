{
  "name": "blanchard",
  "version": "1.0.0",
  "description": "На нашем сайте вы можете ознакомиться с примерами работ авторов, а также почитать интересные статьи",
  "main": "gulpfile.babel.js",
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "html-check": "node-w3c-validator -i src/**/*.html -f lint -b 2048",
    "clean": "gulp cleanRoot",
    "image:svg": "gulp svgOptimization",
    "image:jpg": "gulp imageOptimizationJpg",
    "image:png": "gulp imageOptimizationPng",
    "image:fav": "gulp imageOptimizationFav",
    "build:dev": "gulp build",
    "build:prod": "gulp build --prod",
    "build:watch": "gulp watch",
    "build:full-start": "gulp fullStart",
    "build:full-server": "gulp fullStartServer",
    "build:archive": "gulp archivingFiles",
    "build:backend": "gulp buildBackend",
    "build:cache": "gulp buildCache",
    "lint:editorconfig": "editorconfig-checker"
  },
  "lint-staged": {
    "*.html": "htmlhint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SineYlo/blanchard.git"
  },
  "keywords": [
    "галерея",
    "искусство",
    "арт",
    "художники",
    "примеры работ"
  ],
  "author": "SineYlo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SineYlo/blanchard/issues"
  },
  "homepage": "https://github.com/SineYlo/blanchard#readme",
